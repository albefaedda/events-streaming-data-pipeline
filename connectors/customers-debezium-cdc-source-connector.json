{
  "config": {
    "connector.class": "MySqlCdcSourceV2",
    "name": "MySqlCdcSource_customers",
    "kafka.auth.mode": "KAFKA_API_KEY",
    "kafka.api.key": "<API_KEY>",
    "kafka.api.secret": "<API_SECRET>",
    "database.hostname": "<DB_HOSTNAME>",
    "database.port": "<DB_PORT>",
    "database.user": "<DB_USERNAME>",
    "database.password": "<DB_PASSWORD>",
    "database.ssl.mode": "disabled",
    "output.data.format": "AVRO",
    "output.key.format": "STRING",
    "json.output.decimal.format": "NUMERIC",
    "after.state.only": "true",
    "tombstones.on.delete": "true",
    "topic.prefix": "demo",
    "schema.history.internal.kafka.topic": "dbhistory.demo.ecommerce.schemas",
    "snapshot.mode": "initial",
    "snapshot.locking.mode": "minimal",
    "database.include.list": "ecommerce",
    "table.include.list": "ecommerce.customers",
    "event.processing.failure.handling.mode": "warn",
    "schema.name.adjustment.mode": "avro",
    "field.name.adjustment.mode": "none",
    "heartbeat.interval.ms": "0",
    "inconsistent.schema.handling.mode": "warn",
    "schema.history.internal.skip.unparseable.ddl": "true",
    "schema.history.internal.store.only.captured.tables.ddl": "true",
    "value.converter.ignore.default.for.nullables": "false",
    "schema.context.name": "default",
    "decimal.handling.mode": "double",
    "time.precision.mode": "connect",
    "tasks.max": "1",
    "transforms": "createKey,extractString,mask,renameSchema",
    "transforms.createKey.type": "org.apache.kafka.connect.transforms.ValueToKey",
    "transforms.createKey.fields": "id",
    "transforms.extractString.type": "org.apache.kafka.connect.transforms.ExtractField$Key",
    "transforms.extractString.field": "id",
    "transforms.extractString.field.syntax.version": "V1",
    "transforms.extractString.replace.null.with.default": "true",
    "transforms.mask.type": "org.apache.kafka.connect.transforms.MaskField$Value",
    "transforms.mask.fields": "card_number",
    "transforms.mask.replace.null.with.default": "true",
    "transforms.mask.replacement": "**** **** **** ****",
    "transforms.renameSchema.type": "org.apache.kafka.connect.transforms.SetSchemaMetadata$Value",
    "transforms.renameSchema.schema.name": "com.training.ecommerce.model.Customer",
    "transforms.renameSchema.schema.version": "1"
  }
}